<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="connectholland_sulu_admin.admin" class="ConnectHolland\Sulu\DashboardBundle\Admin\Admin">
            <argument>%sulu_admin.name%</argument>

            <tag name="sulu.admin"/>
        </service>

        <service id="example_news.manager" class="Connectholland\Sulu\DashboardBundle\News\NewsManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
        </service>
    </services>
</container>
